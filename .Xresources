!! fonts
urxvt.font:	xft:Termsyn:pixelsize=12
!xft*antialias:	true
!xft*hinting:	true
!xft*hintstyle:	hintslight
!xft*rgba:		rgba
urxvt*scrollBar: false

URxvt.geometry:	90x25
URxvt*inheritPixmap: true
URxvt*tintColor: gray30
URxvt.internalBorder: 0
URxvt.jumpScroll: true
